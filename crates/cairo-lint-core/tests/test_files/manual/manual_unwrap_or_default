//! > manual unwrap or default for if let with array!

//! > cairo_code
fn main() {
  let x: Option<Array<u128>> = Option::Some(array![1, 2, 3, 4, 5]);
  if let Option::Some(v) = x {
    v
   } else {
     array![]
  };
}

