//! > assert expressions

//! > cairo_code
fn main() {
    assert!(((4)) == 4);
}

//! > diagnostics
warning: Plugin diagnostic: unnecessary double parentheses found. Consider removing them.
 --> lib.cairo:2:13
  |
2 |     assert!(((4)) == 4);
  |             -----
  |

//! > fixed
fn main() {
    assert!(4== 4);
}

//! > ==========================================================================

//! > double parens in let statement

//! > cairo_code
fn main() {
    let _x = ((10 * 2));
}

//! > diagnostics
warning: Plugin diagnostic: unnecessary double parentheses found. Consider removing them.
 --> lib.cairo:2:14
  |
2 |     let _x = ((10 * 2));
  |              ----------
  |

//! > fixed
fn main() {
    let _x = 10 * 2;
}

//! > ==========================================================================

//! > double parens in match arm

//! > cairo_code
fn main() -> felt252 {
    let x = 5;
    match x {
        1 => ((10)),
        5 => ((20)),
        _ => ((30)),
    }
}

//! > diagnostics
warning: Plugin diagnostic: unnecessary double parentheses found. Consider removing them.
 --> lib.cairo:6:14
  |
6 |         1 => ((10)),
  |              ------
  |
warning: Plugin diagnostic: unnecessary double parentheses found. Consider removing them.
 --> lib.cairo:8:14
  |
8 |         5 => ((20)),
  |              ------
  |
warning: Plugin diagnostic: unnecessary double parentheses found. Consider removing them.
  --> lib.cairo:10:14
   |
10 |         _ => ((30)),
   |              ------
   |

//! > fixed
fn main() -> felt252 {
    let x = 5;
    match x {
        1 => 10,
        5 => 20,
        _ => 30,
    }
}

//! > ==========================================================================

//! > double parens in struct field access

//! > cairo_code
struct MyStruct {
    x: felt252,
    y: felt252,
}

fn main() -> felt252 {
    let my_struct = MyStruct { x: 10, y: 20 };
    return ((my_struct.y));
}

//! > diagnostics
warning: Plugin diagnostic: unnecessary double parentheses found. Consider removing them.
  --> lib.cairo:14:12
   |
14 |     return ((my_struct.y));
   |            ---------------
   |

//! > fixed
struct MyStruct {
    x: felt252,
    y: felt252,
}

fn main() -> felt252 {
    let my_struct = MyStruct { x: 10, y: 20 };
    return my_struct.y;
}

