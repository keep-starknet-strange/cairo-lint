//! > loop match pop front with comment in some

//! > cairo_code
fn main() {
    let mut a: Span<u32> = array![1, 2, 3, 4, 5].span();
    loop {
        match a.pop_front() {
            Option::Some(val) => {
                // This is a comment
                println!("{val}")
            },
            Option::None => { 
                break;
            },
        }
    }
}

