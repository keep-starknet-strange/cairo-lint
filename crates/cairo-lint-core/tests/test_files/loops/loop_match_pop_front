//! > simple loop match pop front impl path

//! > cairo_code
use core::array::SpanImpl;
fn main() {
    let mut a: Span<u32> = array![1, 2, 3, 4, 5].span();
    loop {
        match SpanImpl::pop_front(ref a) {
            Option::Some(val) => println!("{val}"),
            Option::None => { break; },
        }
    }
}

