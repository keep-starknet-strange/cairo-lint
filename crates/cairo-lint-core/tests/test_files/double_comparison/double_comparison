//! > double comparison equal and greater than

//! > cairo_code
fn main() -> bool {
    let x = 5_u32;
    let y = 10_u32;
    if x == y || x > y {
        true
    } else {
        false
    }
}

//! > diagnostics
warning: Plugin diagnostic: redundant double comparison found. Consider simplifying to a single comparison.
 --> lib.cairo:4:8
    if x == y || x > y {
       ^*************^

//! > fixed
fn main() -> bool {
    let x = 5_u32;
    let y = 10_u32;
    if x >= y {
        true
    } else {
        false
    }
}

